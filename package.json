    "@antfu/eslint-config": "2.9.0",
    "@iconify-json/carbon": "1.1.31",
    "@types/node": "20.11.30",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "0.58.8",
    "@vitejs/plugin-vue": "5.0.4",
